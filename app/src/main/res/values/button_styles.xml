<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LargePrimary" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
        <item name="shapeAppearance">@style/LargePrimaryButtonShape</item>
        <item name="android:textAppearance">@style/PrimaryMediumSemiBoldCenter</item>
    </style>


    <style name="LargePrimaryButtonShape" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/_2sdp</item>
    </style>
</resources>